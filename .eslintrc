{
    "extends": "airbnb",
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      ecmaVersion: 7
      },
    "rules": {
      "max-len": [0],
      "linebreak-style": [0],
      "quotes": [2, "backtick"],
      "react/prop-types": [0],
      "react/require-extension": "off",
    },
    "settings": {
      "import/resolver": "webpack"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
